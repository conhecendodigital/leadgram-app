{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.formulareal.online"
        }
      ],
      "destination": "https://formulareal.online/:path*",
      "permanent": true
    }
  ],
  "crons": [
    {
      "path": "/api/cron/sync-instagram",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/refresh-tokens",
      "schedule": "0 2 * * *"
    }
  ]
}
